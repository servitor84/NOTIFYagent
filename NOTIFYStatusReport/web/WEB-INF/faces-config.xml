<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="1.2" 
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">

    <!-- MANAGED BEANS -->
    <managed-bean>
        <description>Bean for keeping track of the user session and login status</description>
        <managed-bean-name>userSession</managed-bean-name>
        <managed-bean-class>de.di.notifyagent.subscription.session.UserSession</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
          <property-name>elo</property-name>
          <value>#{eloBean}</value>
        </managed-property></managed-bean>
    <managed-bean>
        <description>Bean to handle the login process</description>
        <managed-bean-name>loginForm</managed-bean-name>
        <managed-bean-class>de.di.notifyagent.subscription.session.LoginForm</managed-bean-class>
    
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <description>Bean to handle the ELO connection</description>
        <managed-bean-name>eloBean</managed-bean-name>
        <managed-bean-class>de.di.notifyagent.subscription.session.EloBean</managed-bean-class>
    
        <managed-bean-scope>none</managed-bean-scope></managed-bean>
    <managed-bean>
        <description>Application bean providing access to the configuration</description>
        <managed-bean-name>application</managed-bean-name>
        <managed-bean-class>de.di.notifyagent.subscription.app.Application</managed-bean-class>
    
        <managed-bean-scope>application</managed-bean-scope></managed-bean>
    <managed-bean>
        <description>Backing bean to manage subscriptions</description>
        <managed-bean-name>subscriptions</managed-bean-name>
        <managed-bean-class>de.di.notifyagent.subscription.app.Subscriptions</managed-bean-class>
    
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
          <property-name>userSession</property-name>
          <value>#{userSession}</value>
        </managed-property></managed-bean>
    <managed-bean>
        <description>Backing bean for a new subscription</description>
        <managed-bean-name>newSubscription</managed-bean-name>
        <managed-bean-class>de.di.notifyagent.subscription.app.NewSubscription</managed-bean-class>
    
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
          <property-name>userSession</property-name>
          <value>#{userSession}</value>
        </managed-property></managed-bean>
    <managed-bean>
        <managed-bean-name>editSubscription</managed-bean-name>
        <managed-bean-class>de.di.notifyagent.subscription.app.EditSubscription</managed-bean-class>
    
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
          <property-name>userSession</property-name>
          <value>#{userSession}</value>
        </managed-property></managed-bean>

